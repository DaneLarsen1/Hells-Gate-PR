<div class="all-PRs-container">
  <div class="card-header">
    <h2>Feed</h2>
  </div>  

  <p>testing</p>

  {{#each lifts as |lift|}}
    <div class="lift">
      
      <p>
        {{lift.title}}
        {{lift.description}}
      </p>
    </div>
    <div class="col-md-7">
      <p>
        {{!-- Uses the User model alias 'user' to get the username attribute that was included in the query for Lift.findAll --}}
        Posted by:
        {{lift.user.username}} 
      </p>
    </div>
  {{/each}}
</div>